<UserControl x:Class="autoplaysharp.App.UI.Tasks.AllianceBattle.AllianceBattleSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContext="{Binding AllianceBattle, Source={StaticResource SettingsViewModels}}">
    <StackPanel>
        <CheckBox Content="Run Normal Mode?" IsChecked="{Binding Settings.RunNormalMode}"/>
        <CheckBox Content="Run Extreme Mode?" IsChecked="{Binding Settings.RunExtremeMode}"/>
    </StackPanel>
</UserControl>
